{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "linebreak-style": "off",
        "indent": ["error", 4],
        "no-console":"off",
        "comma-dangle": "off",
        "implicit-arrow-linebreak":"off",
        "function-paren-newline": "off",
        "object-curly-newline":"off"
    }
}